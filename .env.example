# Cyberwave Edge Configuration Example
# Copy this file to .env and fill in your values

# Cyberwave API Configuration
CYBERWAVE_TOKEN=your_token_here
CYBERWAVE_BASE_URL=https://api.cyberwave.com

# MQTT Configuration (optional, will use base_url host if not set)
# CYBERWAVE_MQTT_HOST=mqtt.cyberwave.com
# CYBERWAVE_MQTT_PORT=1883
# CYBERWAVE_MQTT_USERNAME=
# CYBERWAVE_MQTT_PASSWORD=

# Device Configuration
CYBERWAVE_EDGE_UUID=edge-device-001
CYBERWAVE_TWIN_UUID=your_twin_uuid_here

# Camera Configuration
CAMERA_ID=0
CAMERA_FPS=10

# Logging
LOG_LEVEL=INFO
